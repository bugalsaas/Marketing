{
  "name": "bugal-marketing-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "prisma generate && next build --no-lint",
    "build:analyze": "ANALYZE=true npm run build",
    "build:static": "node scripts/prepare-static-build.js && BUILD_TARGET=static prisma generate && cp next.config.static.ts next.config.ts && next build --no-lint && node scripts/generate-static-data.js && node scripts/restore-routes.js",
    "build:admin": "BUILD_TARGET=admin prisma generate && next build --no-lint",
    "build:all": "npm run build:static && npm run build:admin",
    "start": "next start",
    "start:admin": "BUILD_TARGET=admin next start",
    "lint": "eslint",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "generate:static-data": "node scripts/generate-static-data.js",
    "deploy:static": "npm run build:static && npm run generate:static-data",
    "deploy:admin": "npm run build:admin",
    "deploy:aws:static": "chmod +x scripts/aws-deploy-static.sh && ./scripts/aws-deploy-static.sh",
    "deploy:aws:admin": "chmod +x scripts/aws-deploy-admin.sh && ./scripts/aws-deploy-admin.sh",
    "docker:admin:build": "docker build -f Dockerfile.admin -t bugal-admin .",
    "docker:admin:run": "docker-compose -f docker-compose.admin.yml up",
    "docker:admin:stop": "docker-compose -f docker-compose.admin.yml down"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/nodemailer": "^7.0.2",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^27.0.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.6",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "shiki": "^3.13.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "prisma": "^6.15.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vercel": "^46.1.1"
  }
}
